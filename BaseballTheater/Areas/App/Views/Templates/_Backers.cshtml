<template id="backer">
	<div class="backer">
		<div class="backer">
			<i class="fa fa-beer" aria-hidden="true" v-if="backer.isBeerBacker"></i> {{backer.backerName}}
		</div>
	</div>
</template>

<template id="team-sponsor">
	<a class="team-sponsor-team"
	   v-bind:class="{ 'has-fans' : getTeamSponsorsCount(teamSponsorTeam.team) > 0 }"
	   v-bind:data-team-code="teamSponsorTeam.team"
	   href="https://www.patreon.com/bePatron?u=5206592"
	   target="_blank">
		
		<div class="team-logo dark" v-bind:class="getTeamCode(teamSponsorTeam.team)">
		</div>
		<div class="fan-column">
			<div v-if="teamSponsorTeam.backers.length === 0">
				No fans
			</div>
			<div v-else>
				<h3>
					{{getTeamSponsors(teamSponsorTeam.team)}}
				</h3>
				<template v-for="teamSponsorBacker in teamSponsorTeam.backers">
					<span>{{teamSponsorBacker}}</span>
				</template>
			</div>
		</div>

	</a>
</template>

<template id="premium-sponsor">
	<div class="backer">
		<template v-for="premiumSponsorBacker in premiumSponsorTeam.backers">
			{{premiumSponsorBacker.backerName}}
		</template>
	</div>
</template>